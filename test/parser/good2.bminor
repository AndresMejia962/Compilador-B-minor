// Ejemplo de anidamiento: bucle for dentro de while

main: function void () = {
    contador_externo: integer = 0;
    while (contador_externo < 3) {
        contador_interno: integer;
        for (contador_interno=0; contador_interno < 2; ++contador_interno) {
            print "i=", contador_externo, ", j=", contador_interno;
        }
        
        ++contador_externo;
    }
}