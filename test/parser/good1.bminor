// Ejemplo de bucle do-while con operador de pre-incremento

main: function integer () = {
    contador_ascendente: integer = 0;
    
    do {
        print "Contando hacia arriba: ", contador_ascendente;
        ++contador_ascendente;
    } while (contador_ascendente < 3);
    
    return 0;
}