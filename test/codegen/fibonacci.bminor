calcular_fibonacci: function integer ( posicion: integer ) = {
    if ( posicion <= 1 ) {
        return 1;
    }
    
    return calcular_fibonacci( posicion - 1 ) + calcular_fibonacci( posicion - 2 );
}

main: function void () = {
    numero_prueba: integer = 7;
    resultado_fib: integer = calcular_fibonacci( numero_prueba );

    print "fibonacci(";
    print numero_prueba;
    print ") = ";
    print resultado_fib;
}