fibonacci: function integer ( n: integer ) = {
    if ( n <= 1 ) {
        return 1;
    }
    
    return fibonacci( n - 1 ) + fibonacci( n - 2 );
}

main: function void () = {
    n_test: integer = 7;
    result: integer = fibonacci( n_test );

    print "fibonacci(";
    print n_test;
    print ") = ";
    print result;
}